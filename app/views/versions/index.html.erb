<h1>Versions</h1>

<table class='table-striped'>
  <tr>
    <th>Version</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @versions.each do |version| %>
  <tr>
    <td><%= version.version %></td>
    <td><%= link_to 'Show', version %></td>
    <td><%= link_to 'Edit', edit_version_path(version) %></td>
    <td><%= button_to 'Destroy', version, :confirm => 'Are you sure?', :method => :delete, :class => 'btn-small'%></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Version', new_version_path %>
